        $(document).ready(function() {
            $(window).on('scroll', function(event) {
                if ($(window).scrollTop() < $('.g-leftls').offset().top) {
                    $('.g-leftls').css({
                        position: 'absolute',
                        top: '76px'
                    });
                }
                if ($(window).scrollTop() > $('.g-leftls').offset().top - $('.header').innerHeight()) {
                    $('.g-leftls').css({
                        position: 'fixed',
                        top: $('.header').innerHeight()
                    });
                }
                if ($(window).scrollTop() > $(document).innerHeight() - $('.footer').innerHeight() - 900) {
                    $('.g-leftls').css({
                        position: 'absolute',
                        top: 'auto',
                        bottom: '146px'
                    });
                }
            });
        });
